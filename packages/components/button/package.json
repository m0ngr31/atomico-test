{
  "name": "@assemble-web/button",
  "version": "1.0.0",
  "type": "module",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "dependencies": {
    "atomico": "*"
  },
  "peerDependencies": {
    "@atomico/react": "*",
    "@atomico/vue": "*",
    "atomico": "*"
  },
  "peerDependenciesMeta": {
    "@atomico/react": {
      "optional": true
    },
    "@atomico/vue": {
      "optional": true
    }
  },
  "files": [
    "lib",
    "types",
    "wrappers"
  ],
  "scripts": {
    "build:types": "tsc",
    "build:compile": "library \"src/**/*\"",
    "build:exports": "exports \"lib/**/*\" \"types/**/*\" --wrappers",
    "build": "npm run build:types && npm run build:compile && npm run build:exports",
    "working:types": "tsc --watch",
    "working:compile": "library \"src/**/*\" --watch",
    "working:exports": "exports \"lib/**/*\" \"types/**/*\" --wrappers --watch",
    "dev": "npm run build:types; npm run build:compile; npm run working:types & npm run working:compile & npm run working:exports",
    "prepublishOnly": "npm run build"
  },
  "types": "types/index.d.ts",
  "exports": {
    "./element": {
      "types": "./types/element.d.ts",
      "default": "./lib/element.js"
    },
    ".": {
      "types": "./types/index.d.ts",
      "default": "./lib/index.js"
    },
    "./stories/index.stories": "./src/stories/index.stories.tsx",
    "./react": {
      "types": "./wrapper/react.d.ts",
      "default": "./wrapper/react.js"
    },
    "./preact": {
      "types": "./wrapper/preact.d.ts",
      "default": "./wrapper/preact.js"
    },
    "./vue": {
      "types": "./wrapper/vue.d.ts",
      "default": "./wrapper/vue.js"
    }
  },
  "typesVersions": {
    "*": {
      "react": [
        "wrapper/react.d.ts"
      ],
      "element": [
        "types/element.d.ts"
      ],
      "preact": [
        "wrapper/preact.d.ts"
      ],
      "vue": [
        "wrapper/vue.d.ts"
      ]
    }
  }
}